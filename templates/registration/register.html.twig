{# {% extends 'base.html.twig' %} #}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        {% block stylesheets %}
            <link href={{ asset("css/registrationForm.css") }} rel="stylesheet">
        {% endblock %}
    </head>
    <body>
    {% block body %}
        <div class="container">
            <div class="row">
                <div class="bg-light mt-4 p-4" id="regForm">
                    <h1>Création de votre compte licencié</h1> <br>
                    <h3>
                    {{ form_start(registrationForm) }}
                        {{ form_row(registrationForm.numlicence, {'label' : 'Votre numéro de licence : '}) }} <br>
                        {{ form_row(registrationForm.email, {'label' : 'Votre adresse email : '}) }} <br>
                        {{ form_row(registrationForm.plainPassword) }} <br>
                        <br> <button type="submit" class="btn btn-primary" id="bouttonEnregistrement">S'enregistrer</button>
                    {{ form_end(registrationForm) }}
                    </h3>
                </div>
            </div>
        </div>
    {% endblock %}
    </body>
</html>